{
    "rules": {
      "missing-jsdoc": [false],
      "no-unsafe-any": [false],
      "no-relative-imports": [false],
      "no-multiline-string": [false],
      "completed-docs": [false],
      "no-import-side-effect": [false],
      "strict-boolean-expressions": [false],
      "import-name": false,
      "one-variable-per-declaration": [true],
      "prefer-const": true,
      "prefer-type-cast": false,
      "no-for-in-array": true,
      "no-unsafe-finally": true,
      "object-literal-key-quotes": [true, "as-needed"],
      "only-arrow-functions": [false],
      "ordered-imports": [true],
      "restrict-plus-operands": false,
      "max-file-line-count": [true, 500],
      "arrow-parens": true,
  
      "export-name": [false, ".*ChartOptions", "manifest"],
      "member-access": true,
      "no-any": true,
      "no-namespace": true,
      "no-sparse-arrays": true,
      "use-isnan": true,
  
      "align": [true, "parameters", "arguments", "statements"],
      "ban": true,
      "class-name": true,
      "comment-format": [false, "check-space", "check-lowercase"],
      "curly": true,
      "deprecation": true,
      "eofline": false,
      "forin": true,
      "indent": [true, "spaces"],
      "interface-name": [true, "always-prefix"],
      "jsdoc-format": true,
      "label-position": true,
      "linebreak-style": [false],
      "max-line-length": [true, 140],
      "member-ordering": [
        true,
        {
          "order": [
            "static-field",
            "instance-field",
            "constructor",
  
            "protected-instance-method",
            "private-instance-method",
            "public-instance-method"
          ]
        }
      ],
      "new-parens": true,
      "no-angle-bracket-type-assertion": false,
      "no-arg": true,
      "no-bitwise": true,
      "no-conditional-assignment": true,
      "no-consecutive-blank-lines": true,
      "no-console": [true, "time", "timeEnd", "trace"],
      "no-construct": true,
      "no-debugger": true,
      "no-default-export": true,
      "no-duplicate-variable": true,
      "no-empty": true,
      "no-eval": true,
      "no-inferrable-types": false,
      "no-internal-module": true,
      "no-mergeable-namespace": true,
      "no-null-keyword": false,
      "no-reference": true,
      "no-require-imports": false,
      "no-shadowed-variable": true,
      "no-string-literal": true,
      "no-switch-case-fall-through": true,
      "no-trailing-whitespace": true,
      "no-unused-expression": true,
      "no-use-before-declare": true,
      "no-var-keyword": true,
      "no-var-requires": true,
      "object-literal-sort-keys": true,
      "one-line": [true, "check-open-brace", "check-catch", "check-else", "check-whitespace"],
      "quotemark": [true, "single", "avoid-escape", "avoid-template"],
      "radix": true,
      "semicolon": [true, "always", "ignore-interfaces"],
      "switch-default": true,
      "trailing-comma": [true, {"multiline": {"arrays": "always", "objects": "always", "functions": "never"}, "singleline": "never"}],
      "triple-equals": [true, "allow-null-check"],
      "typedef": [true, "call-signature", "parameter", "property-declaration", "variable-declaration", "member-variable-declaration"],
      "typedef-whitespace": [true, {"call-signature": "nospace", "index-signature": "nospace", "parameter": "nospace", "property-declaration": "nospace", "variable-declaration": "nospace"}],
      "variable-name": true,
      "whitespace": [true, "check-branch", "check-decl", "check-module", "check-operator", "check-separator", "check-type"]
    }
  }
  